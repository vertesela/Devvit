{
  "name": "@devvit/runtimes",
  "version": "0.10.19",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "main": "./index.js",
  "scripts": {
    "build": "tsc && yarn build:workers --sourcemap=linked && yarn build:polyfills",
    "build:polyfills": "./scripts/build-polyfills.sh",
    "build:workers": "./scripts/build-workers.sh",
    "clean": "rm -rf .turbo coverage dist .devvitLocalStorage",
    "clobber": "yarn clean && rm -rf node_modules",
    "coverage": "vitest run --coverage",
    "dev": "concurrently -n 'lib,workers' 'tsc -w' 'yarn build:workers --watch --sourcemap=inline'",
    "lint": "redlint .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "rm -rf dist/test-util dist/test-actors && publish-package-json",
    "test": "yarn test:unit && yarn test:types",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit-with-coverage": "NODE_OPTIONS=\"--max-old-space-size=8192\" vitest run --coverage"
  },
  "types": "./index.d.ts",
  "dependencies": {
    "@devvit/protos": "0.10.19",
    "@devvit/shared-types": "0.10.19",
    "@devvit/web-worker": "0.10.19",
    "base64-js": "1.5.1",
    "buffer": "6.0.3",
    "cron-parser": "4.7.1",
    "grpc-web": "1.4.2",
    "lru-cache": "7.10.1",
    "node-localstorage": "2.2.1",
    "redis": "4.6.6",
    "rxjs": "7.8.1",
    "undici": "5.28.2",
    "uuid": "9.0.0",
    "ws": "8.11.0"
  },
  "devDependencies": {
    "@devvit/eslint-config": "0.10.19",
    "@devvit/repo-tools": "0.10.19",
    "@devvit/tsconfig": "0.10.19",
    "@types/node-localstorage": "1.3.0",
    "@types/redis-mock": "0.17.1",
    "@types/uuid": "9.0.0",
    "@types/ws": "8.5.3",
    "concurrently": "7.5.0",
    "esbuild": "0.18.12",
    "eslint": "8.9.0",
    "portfinder": "1.0.32",
    "redis-mock": "0.56.3",
    "typescript": "5.3.2",
    "uuid": "9.0.0",
    "vitest": "0.31.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "source": "./src/index.ts",
  "gitHead": "ff950a914e523b8b8a2a080df02e6f5cdab7891e"
}
