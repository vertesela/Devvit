{
  "name": "devvit",
  "version": "0.10.17",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./bin/devvit": "./bin/devvit.js"
  },
  "main": "./dist/index.js",
  "bin": "bin/devvit.js",
  "files": [
    "./bin",
    "./dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo dist",
    "clobber": "yarn clean && rm -rf node_modules",
    "dev": "tsc -w",
    "lint": "yarn exec redlint .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "publish-package-json",
    "test": "yarn test:unit && yarn test:types && yarn lint",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit-with-coverage": "vitest run --coverage"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@devvit/cli": "0.10.17"
  },
  "devDependencies": {
    "@devvit/eslint-config": "0.10.17",
    "@devvit/repo-tools": "0.10.17",
    "@devvit/tsconfig": "0.10.17",
    "eslint": "8.9.0",
    "typescript": "5.3.2",
    "vitest": "0.31.0"
  },
  "source": "./src/index.ts",
  "gitHead": "0563739097f90f2a45c8854873a355a79272660e"
}
