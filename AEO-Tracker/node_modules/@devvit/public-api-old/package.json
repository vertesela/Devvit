{
  "name": "@devvit/public-api-old",
  "version": "0.10.10",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "main": "./index.js",
  "scripts": {
    "build": "yarn run:scripts && yarn build:icon-types && tsc",
    "build:icon-types": "make-icons src/jsx/icons.ts",
    "clean": "rm -rf .turbo coverage dist src/abstractions/*Devvit.ts src/jsx/icons.ts || :",
    "clobber": "yarn clean && rm -rf node_modules",
    "dev": "tsc -w",
    "lint": "echo 'Not bothering to lint the old API. Run `yarn lint:really` if you want to anyway.'",
    "lint:fix": "yarn lint:really --fix",
    "lint:really": "redlint .",
    "prepublishOnly": "publish-package-json && scripts/clean-internals.sh",
    "run:scripts": "node scripts/generateDevvitShim.js && npx prettier -w src/abstractions/*Devvit.ts",
    "test": "yarn test:unit && yarn test:types",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit-with-coverage": "vitest run --coverage"
  },
  "types": "./index.d.ts",
  "dependencies": {
    "@devvit/protos": "0.10.10",
    "@devvit/runtimes": "0.10.10",
    "@devvit/shared-types": "0.10.10",
    "base64-js": "1.5.1"
  },
  "devDependencies": {
    "@devvit/eslint-config": "0.10.10",
    "@devvit/repo-tools": "0.10.10",
    "@devvit/tsconfig": "0.10.10",
    "@types/clone-deep": "4.0.1",
    "clone-deep": "4.0.1",
    "eslint": "8.9.0",
    "typescript": "5.3.2",
    "vitest": "0.31.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "source": "./src/index.ts",
  "gitHead": "f0464ae2f093625d745a84e1fc873c701f3a7e52"
}
