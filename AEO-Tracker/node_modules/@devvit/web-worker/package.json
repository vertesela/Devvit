{
  "name": "@devvit/web-worker",
  "version": "0.10.17",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "exports": {
    "browser": "./cjs/browser.cjs",
    "node": "./cjs/node.cjs",
    "default": "./cjs/node.cjs"
  },
  "main": "./cjs/node.cjs",
  "browser": "./cjs/browser.cjs",
  "files": [
    "cjs/**",
    "index.d.ts"
  ],
  "scripts": {
    "build": "babel src/node.cjs src/browser.cjs -d cjs --out-file-extension .cjs",
    "clean": "rm -rf cjs dist .turbo",
    "clobber": "yarn clean && rm -rf node_modules",
    "dev": "yarn build",
    "lint": "eslint -c .eslintrc.js \"src/*.cjs\" \"test/**\"",
    "lint:fix": "yarn lint --fix",
    "test": "yarn test:unit && yarn test:types && yarn lint",
    "test:types": "echo 'no type checking yet'",
    "test:unit": "vitest",
    "test:unit-with-coverage": "vitest run --coverage"
  },
  "types": "index.d.ts",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.21.5",
    "@babel/preset-env": "^7.7.7",
    "@devvit/tsconfig": "0.10.17",
    "babel-plugin-commonjs-lite": "^0.1.0",
    "eslint": "7.26.0",
    "eslint-config-developit": "1.2.0",
    "vitest": "0.31.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "babel-plugin-commonjs-lite"
    ]
  },
  "source": "./src/index.ts",
  "gitHead": "0563739097f90f2a45c8854873a355a79272660e"
}
