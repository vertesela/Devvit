{
  "name": "@devvit/ui-renderer",
  "version": "0.10.17",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "main": "./index.js",
  "scripts": {
    "build": "yarn library:build",
    "clean": "rm -rf .turbo coverage dist library/src/semanticColors.ts",
    "clobber": "yarn clean && rm -rf node_modules",
    "dev": "yarn library:dev",
    "docker:test:ui": "bash visual-testing/test-in-docker.sh",
    "docker:visual-test:approve": "bash visual-testing/approve.sh",
    "library:build": "yarn library:build:semantic-colors && yarn library:build:typescript && yarn library:build:styles",
    "library:build:custom-styles": "cd library && postcss custom-styles.css -o ../dist/custom-styles.css",
    "library:build:semantic-colors": "node scripts/make-semantic-colors.js",
    "library:build:styles": "cd library && postcss styles.css -o tailwind-build.css && yarn library:build:styles-dist && yarn library:build:custom-styles",
    "library:build:styles-dist": "node scripts/make-styles.js",
    "library:build:typescript": "tsc -p library",
    "library:dev": "concurrently -n 'lib:styles,lib:styles-dist,lib:typescript' 'yarn library:dev:styles' 'yarn library:dev:styles-dist' 'yarn library:dev:typescript",
    "library:dev:styles": "yarn library:build:styles -w",
    "library:dev:styles-dist": "yarn library:build:styles-dist --watch",
    "library:dev:typescript": "yarn library:build:typescript -w",
    "library:lint": "redlint ./library",
    "library:lint:fix": "yarn library:lint --fix",
    "library:test": "yarn library:test:unit && yarn library:test:types && yarn library:lint",
    "library:test:types": "tsc --noEmit -p library",
    "library:test:unit": "vitest run -r library -c vitest.config.js",
    "library:test:unit-with-coverage": "yarn library:test:unit --coverage",
    "lint": "yarn library:lint && yarn lint:circular-deps",
    "lint:circular-deps": "yarn madge --circular dist/**/*.js",
    "lint:fix": "yarn library:lint:fix",
    "prepublishOnly": "publish-package-json",
    "test": "yarn library:test",
    "test:types": "yarn library:test:types",
    "test:ui": "bash ./scripts/install-chrome.sh; yarn visual-test:build && yarn visual-test:check && concurrently -k -s first 'yarn visual-test:dev:serve' 'sleep 5; yarn visual-test:test'",
    "test:unit": "yarn library:test:unit",
    "test:unit-with-coverage": "yarn library:test:unit-with-coverage",
    "visual-test:approve": "backstop --config backstop.config.cjs approve",
    "visual-test:build": "yarn visual-test:build:typescript && yarn visual-test:build:vite && yarn visual-test:build:layouts",
    "visual-test:build:layouts": "node scripts/build-layouts.js",
    "visual-test:build:typescript": "tsc -p visual-testing",
    "visual-test:build:vite": "vite build --force --config visual-testing/vite.config.js",
    "visual-test:check": "bash ./scripts/check-layouts.sh",
    "visual-test:dev": "concurrently -n 'viz:typescript,viz:vite,viz:serve' 'yarn visual-test:dev:typescript' 'yarn visual-test:dev:vite' 'yarn visual-test:dev:serve'",
    "visual-test:dev:serve": "http-server ./build -p 5173",
    "visual-test:dev:typescript": "tsc -p visual-testing -w",
    "visual-test:dev:vite": "vite build --force --config visual-testing/vite.config.js --watch",
    "visual-test:report": "backstop --config backstop.config.cjs openReport",
    "visual-test:test": "backstop --config backstop.config.cjs test"
  },
  "types": "./index.d.ts",
  "dependencies": {
    "@devvit/protos": "0.10.17",
    "@devvit/runtime-lite": "0.10.17",
    "@devvit/runtimes": "0.10.17",
    "@devvit/shared-types": "0.10.17",
    "@dotlottie/player-component": "2.7.2",
    "p-queue": "7.3.4",
    "rxjs": "7.8.1"
  },
  "peerDependencies": {
    "@reddit/baseplate": "1.1.3",
    "@reddit/faceplate": "2.x",
    "@reddit/faceplate-ui": "11.x"
  },
  "peerDependenciesMeta": {
    "@reddit/baseplate": {
      "optional": true
    },
    "@reddit/faceplate": {
      "optional": true
    },
    "@reddit/faceplate-ui": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "lit": "^2.0.0"
  },
  "devDependencies": {
    "@devvit/eslint-config": "0.10.17",
    "@devvit/repo-tools": "0.10.17",
    "@devvit/tsconfig": "0.10.17",
    "@lit-labs/ssr": "^2.2.3",
    "@lit/localize": "0.11.4",
    "@open-wc/testing-helpers": "2.3.0",
    "@reddit/baseplate": "1.1.3",
    "@reddit/eslint-plugin-i18n-shreddit": "0.1.0",
    "@types/dompurify": "3.0.5",
    "autoprefixer": "10.4.16",
    "backstopjs": "6.2.1",
    "base64-js": "1.5.1",
    "chokidar": "3.5.3",
    "chrome-launcher": "1.0.0",
    "concurrently": "7.5.0",
    "debounce": "1.2.1",
    "eslint": "8.9.0",
    "fast-xml-parser": "4.2.7",
    "http-server": "14.1.1",
    "lit": "2.2.8",
    "madge": "6.0.0",
    "postcss": "8.4.22",
    "postcss-cli": "10.1.0",
    "postcss-import": "15.1.0",
    "puppeteer": "21.2.1",
    "tailwindcss": "3.3.1",
    "ts-node": "10.9.1",
    "typescript": "5.3.2",
    "vite": "4.5.1",
    "vitest": "0.31.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "source": "./src/index.ts",
  "gitHead": "0563739097f90f2a45c8854873a355a79272660e"
}
