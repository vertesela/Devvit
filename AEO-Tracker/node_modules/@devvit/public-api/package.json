{
  "name": "@devvit/public-api",
  "version": "0.10.17",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "main": "./index.js",
  "scripts": {
    "build": "yarn build:icon-types && yarn build:semantic-colors && tsc && cp -a devvit.tsconfig.json dist/ && yarn build:types",
    "build:icon-types": "make-icons src/types/icons.ts",
    "build:semantic-colors": "node scripts/make-semantic-colors.js",
    "build:types": "api-extractor run && sed -ne '/declare global {/,$ p' src/devvit/Devvit.ts >> dist/public-api.d.ts",
    "clean": "rm -rf .turbo api-extractor coverage dist src/devvit/internals/semanticColors.ts src/types/icons.ts || :",
    "clobber": "yarn clean && rm -rf node_modules",
    "dev": "tsc -w",
    "lint": "redlint .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "publish-package-json",
    "test": "yarn test:unit && yarn test:types",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit-with-coverage": "vitest run --coverage"
  },
  "types": "./index.d.ts",
  "dependencies": {
    "@devvit/protos": "0.10.17",
    "@devvit/runtimes": "0.10.17",
    "@devvit/shared-types": "0.10.17",
    "base64-js": "1.5.1",
    "clone-deep": "4.0.1",
    "core-js": "3.27.2",
    "lodash.isequal": "4.5.0"
  },
  "devDependencies": {
    "@devvit/eslint-config": "0.10.17",
    "@devvit/repo-tools": "0.10.17",
    "@devvit/tsconfig": "0.10.17",
    "@microsoft/api-extractor": "7.41.0",
    "@reddit/faceplate-ui": "11.1.0",
    "@types/clone-deep": "4.0.1",
    "@types/core-js": "2.5.6",
    "@types/lodash.isequal": "4.5.6",
    "@vitest/coverage-c8": "0.32.0",
    "eslint": "8.9.0",
    "typescript": "5.3.2",
    "vitest": "0.31.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "source": "./src/index.ts",
  "gitHead": "0563739097f90f2a45c8854873a355a79272660e"
}
